
<h1>Our Reviewers</h1>

  <div class="row">
    <% users.forEach(user => { %>
      <div class="col s12 m6">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <span class="card-title"> <%= user.username %></span>
              <div class="card-action">
                <a href="/users/<%= user.id %>" class="green-text">View Details</a>
              </div>
          </div>
        </div>
      </div>
    <% }) %>
  </div>
